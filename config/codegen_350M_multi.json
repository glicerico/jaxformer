{
    "debug_mock_data": false,
    "debug_emulate_tpu_on_cpu": false,
    "debug_run_local": false,

    "tpu_user": "asuarez",
    "tpu_version": 3,
    "tpu_image": "v2-alpha",
    "tpu_size": 64,
    "tpu_delete": true,
    "tpu_spawn": true,
    "tpu_reserved": false,
    "tpu_create_env": true,
    "tpu_name": "sfr-{tpu_user}-tpu-v{tpu_version}-{tpu_size}-{tpu_zone}-1",
    "tpu_zone": "us-central1-a",
    "tpu_worker_port": 10000,
    "tpu_internal_ips": false,
    
    "model_type": "decoder",
    "model_sharding": "inter",
    "model_layers": 20,
    "model_dim": 1024,
    "model_head_dim": 64,
    "model_heads": 16,
    "model_vocab_size": 51200,
    "model_pe_rotary_dims": 32,
    "model_seq_len": 2048,

    "opt_warmup_steps": 300,
    "opt_anneal_steps": 14700,
    "opt_total_steps": 15000,
    "opt_lr_max": 1.8e-4,
    "opt_lr_end": 3.0e-5,
    "opt_weight_decay": 0.1,
    "opt_clip_by_global_norm": 1.0,
    "opt_gradient_accumulation_steps": 32,
    "opt_per_replica_batch": 1,
    "opt_params_partitions": 1,

    "data_train_set": "gs://codegen_test0/dataset_v1/*.tfrecords",

    "restore_reinit": true,
    "restore_ckpt": "gs://sfr-codegen-research/checkpoints/codegen-350M-multi",
    "restore_step": 150000,

    "ckpt_dir": "gs://codegen_test0/checkpoints/codegen-350M-multi-amr",
    "ckpt_every": 2000,
    "ckpt_attempt": 0,

    "wandb_enabled": true,
    "wandb_project": "codegen-amr",
    "wandb_entity": "glicerico",
    "wandb_group": "350M",
    "wandb_name": "codegen_350M_multi"
}
